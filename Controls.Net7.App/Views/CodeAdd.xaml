<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Controls.Net7.App.Views.CodeAdd"
             >
    <VerticalStackLayout>
        <TableView  Intent="Form"  IsVisible="true" x:Name="Codessm">
            <TableRoot>
                <TableSection>
                    <EntryCell Label="用处" Text="{Binding Use}"/>
                    <EntryCell Label="用处细节" Text="{Binding UseDetail}"/>
                    <EntryCell Label="代码" Text="{Binding Code}"/>
                </TableSection>
            </TableRoot>
        </TableView>
        <Button Text="确定"  Clicked="Button_Clicked"></Button>
    </VerticalStackLayout>
</ContentPage>